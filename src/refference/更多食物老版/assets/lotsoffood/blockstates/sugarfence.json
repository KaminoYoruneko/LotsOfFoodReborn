{
    "multipart":
	[
		{
			"apply": {"model": "pda:sugarfence_post"}
		},
        {
			"when": {"north": "true"},
            "apply": {"model": "pda:sugarfence_side", "uvlock": true}
        },
        {
			"when": {"east": "true"},
            "apply": {"model": "pda:sugarfence_side", "y": 90, "uvlock": true}
        },
        {
			"when": {"south": "true"},
            "apply": {"model": "pda:sugarfence_side", "y": 180, "uvlock": true}
        },
        {
			"when": {"west": "true"},
            "apply": {"model": "pda:sugarfence_side", "y": 270, "uvlock": true}
        }
   ]
}
